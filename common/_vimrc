set nocompatible
behave xterm
set autoindent
set backspace=1
set expandtab
set ignorecase
set smartcase
set hlsearch
set incsearch
set list
set listchars=tab:>-,trail:-
set noautowrite
set nobackup
set noerrorbells
set nowritebackup
set ruler
set selectmode=mouse
set shiftwidth=4
set showmatch
set tabstop=4
set tags=$HOME\code\tags
set visualbell
set updatecount=0
let g:ctags_statusline=1
